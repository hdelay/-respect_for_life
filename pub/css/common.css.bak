}
/* 정렬 */
.agn_l {text-align: left !important;}
.agn_r {text-align: right !important;}
.agn_c {text-align: center !important;}

/* 마진 */
.mgt_10 {margin-top: 10px !important;}
.mgt_20 {margin-top: 20px !important;}
.mgt_30 {margin-top: 30px !important;}
.mgt_40 {margin-top: 40px !important;}
.mg_0 {margin: 0 !important;}

/* 파일링크 */
.file_link_group li {
    line-height: 20px;
}
.file_link_group li + li {
    margin-top: 5px;
}
.file_link_group a {
    padding-left: 20px;
    position: relative;
}
.file_link_group a::before {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    background: url(../images/ico/ico_file.png) no-repeat left center;
    background-size: 16px;
    width: 20px;
    height: 20px;
}
.file_link_group a:hover {
