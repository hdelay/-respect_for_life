    color: #fff;
}
.big {
    height: 45px;
    padding: 0 60px;
}
.btn_search {
    background-image: url(../images/ico/ico_search.svg);
    background-size: 30px;
    background-repeat: no-repeat;
    background-position: center;
    text-indent: -9999px;
