.login_join .step ul {
    display: flex;
    width: 80%;
    margin: 0 auto;
    justify-content: space-between;
    flex-wrap: wrap;
}
.login_join .step ul li {
    display: flex;
    align-items: center;
    position: relative;
    width: 200px;
    height: 60px;
    border-radius: 60px;
    background-color: #f5f5f5;
    background-repeat: no-repeat;
    background-position: 34px center;
    background-size: 19%;
    padding-left: 80px;
    box-sizing: border-box;
    box-shadow: 5px 5px 5px -3px rgba(0,0,0,0.1);
    margin-top: 15px;
    
}
.login_join .step ul li.on {
    background-color: #EC6818;
    color: #fff;
}
.login_join .step ul li::before {
    content: "";
    display: block;
    position: absolute;
    top: 50%;
    right: -6px;
    margin-top: -15px;
    width: 30px;
    height: 30px;
    transform: rotate(-45deg);
    background-color: #f5f5f5;
}
.login_join .step ul li.on::before {
    background-color: #EC6818;
}
.login_join .step ul li:last-child::before {
    display: none;
}
.login_join .step ul li:nth-child(1) {background-image: url(../images/ico/ico_step01.svg); background-size: 19%;}
.login_join .step ul li:nth-child(2) {background-image: url(../images/ico/ico_step02.svg); background-size: 10%; background-position: 50px center;}
.login_join .step ul li:nth-child(3) {background-image: url(../images/ico/ico_step03.svg); background-size: 17%; background-position: 39px center;}
.login_join .step ul li:nth-child(4) {background-image: url(../images/ico/ico_step04.svg); background-size: 19%;}

.login_join .step ul li.on:nth-child(1) {background-image: url(../images/ico/ico_step01_w.svg); background-size: 19%;}
.login_join .step ul li.on:nth-child(2) {background-image: url(../images/ico/ico_step02_w.svg); background-size: 10%; background-position: 50px center;}
.login_join .step ul li.on:nth-child(3) {background-image: url(../images/ico/ico_step03_w.svg); background-size: 17%; background-position: 39px center;}
.login_join .step ul li.on:nth-child(4) {background-image: url(../images/ico/ico_step04_w.svg); background-size: 19%;}

